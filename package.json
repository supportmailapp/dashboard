{
  "name": "dashboard",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "start": "pnpm run build && pm2 start pm2.config.js --env production",
    "dev": "vite dev",
    "build": "pnpm run update:submodules && vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "update:submodules": "git submodule update --init --recursive"
  },
  "devDependencies": {
    "@internationalized/date": "^3.10.1",
    "@lucide/svelte": "^0.561.0",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.49.3",
    "@sveltejs/vite-plugin-svelte": "^6.2.3",
    "@sveltejs/vite-plugin-svelte-inspector": "^5.0.2",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/jsonwebtoken": "^9.0.10",
    "bits-ui": "^2.15.4",
    "clsx": "^2.1.1",
    "mode-watcher": "^1.1.0",
    "prettier": "3.6.0",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-sonner": "^1.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-plugin-devtools-json": "^1.0.0"
  },
  "dependencies": {
    "@arcjet/inspect": "^1.1.0",
    "@arcjet/sveltekit": "^1.1.0",
    "@discordapp/twemoji": "^16.0.1",
    "@fontsource-variable/geist": "^5.2.8",
    "@fontsource-variable/raleway": "^5.2.8",
    "@sapphire/snowflake": "^3.5.5",
    "@sentry/sveltekit": "^9.47.1",
    "dayjs": "^1.11.19",
    "discord-api-types": "^0.38.37",
    "discord.js": "^14.25.1",
    "fast-deep-equal": "^3.1.3",
