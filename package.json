    "runed": "^0.37.1",
    "socket.io-client": "^4.8.3",
    "wildcard-match": "^5.1.4",
    "zod": "^4.3.5",
    "zod-validation-error": "^5.0.0"
  },
  "packageManager": "pnpm@10.29.3"
}
